APP_NAME=grails
ENV_VAR=GRAILS_HOME
GET_CMD='wget http://dist.springframework.org.s3.amazonaws.com/release/GRAILS/grails-2.1.1.zip'
APP_FILE=grails-2.1.1.zip
EXTRACT_DIR=grails-2.1.1
